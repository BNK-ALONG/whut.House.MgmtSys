<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<!-- 配置数据库连接 -->
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<!-- 配置是否生成注释 -->  
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql:///whlglease" userId="root" password="">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" /> 
		</javaTypeResolver>
 
		<!-- 指定javabean的生成位置 -->

		<javaModelGenerator targetPackage="com.computerdesign.whutHouseMgmt.bean.param.systemParam"
			targetProject=".\src\main\java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!-- 指定sql映射文件的生成位置 -->

		<sqlMapGenerator targetPackage="mapper.param.systemParam" targetProject=".\src\main\resources">
			<property name="enableSubPackages" value="true" /> 
		</sqlMapGenerator>

		<!-- 指定dao接口的生成位置 mapper接口 -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.computerdesign.whutHouseMgmt.dao.param.systemParam" targetProject=".\src\main\java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!-- <table tableName="View_Hs_StaffHouse" domainObjectName="StaffHouse">
