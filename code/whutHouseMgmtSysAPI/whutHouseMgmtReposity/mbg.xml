			<property name="useActualColumnNames" value="true"></property>
		</table> -->
		<!-- <table tableName="pm_rentevent" domainObjectName="RentEvent">
			<property name="useActualColumnNames" value="true"></property>
		</table> -->
		<!-- <table tableName="view_hs_selfhelpselecthouse" domainObjectName="SelfHelpSelectHouse">
			<property name="useActualColumnNames" value="true"></property>
		</table> -->
		<table tableName="Au_Permission" domainObjectName="Permission">
			<property name="useActualColumnNames" value="true"></property>
		</table>
		<table tableName="Au_PermissionClass" domainObjectName="PermissionClass">
			<property name="useActualColumnNames" value="true"></property>
		</table>
		<table tableName="Au_Role" domainObjectName="Role">
			<property name="useActualColumnNames" value="true"></property>
		</table>
		<table tableName="Au_Rule" domainObjectName="Rule">
